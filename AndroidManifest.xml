<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ysulibrary"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
	
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application
        android:name="com.example.ysulibrary.YsuApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ysulibrary.activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.ysulibrary.activities.LoginActivity"></activity>
        <activity android:name="com.example.ysulibrary.activities.ReaderInfoActivity"></activity>
        <activity android:name="com.example.ysulibrary.activities.NowLendActivity"></activity>
        <activity android:name="com.example.ysulibrary.activities.LendHistoryActivity"></activity>
        <activity android:name="com.example.ysulibrary.activities.AsordHistoryActivity"></activity>
        <activity android:name="com.example.ysulibrary.activities.PreBookActivity"></activity>
    </application>

</manifest>
